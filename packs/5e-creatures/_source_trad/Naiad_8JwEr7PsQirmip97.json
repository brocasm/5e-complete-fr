    {
      "name": "Phantasmal Force",
      "type": "spell",
      "img": "https://assets.forge-vtt.com/bazaar/core/icons/creatures/slimes/slime-giant-face-eyes.webp",
      "flags": {
        "srd5e": {
          "page": "spells.html",
          "source": "PHB",
          "hash": "phantasmal%20force_phb",
          "propDroppable": "spell"
