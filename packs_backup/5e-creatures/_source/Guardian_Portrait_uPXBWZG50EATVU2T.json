    {
      "name": "Telekinesis",
      "type": "spell",
      "img": "https://assets.forge-vtt.com/bazaar/core/icons/skills/toxins/cup-goblet-poisoned-spilled.webp",
      "flags": {
        "srd5e": {
          "page": "spells.html",
